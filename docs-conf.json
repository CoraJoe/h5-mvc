{
	"tags": {
		"allowUnknownTags" : true
	},
    "source": {
//        "includePattern": ".+\\.js(doc|x)?$",
        "exclude": [
            "gom/src/3rd"
        ]
    },
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "GomUI",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "Gom GomUI",
			"description": "Gom for Go mobile!",
			"keyword": "Gom, GomUI, go mobile"
		},
		"linenums": false
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples"]
	}
}
